{
  "$id": "https://example.com/complex-object.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Person",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "default": "Rupert"
    },
    "male": {
      "type": "boolean",
      "default": true
    },
    "age": {
      "type": "integer",
      "default": 33,
      "minimum": 0
    },
    "address": {
      "type": "object",
      "properties": {
        "street": {
          "type": "string",
          "default": "Rances Lane"
        },
        "city": {
          "type": "string",
          "default": "Wokingham"
        },
        "state": {
          "type": "string",
          "default": "Berkshire"
        },
        "country": {
          "type": "object",
          "properties": {
            "countryName": {
              "type": "string",
              "default": "England"
            }
          }
        },
        "postalCode": {
          "type": "object",
          "properties": {
            "codePart1": {
              "type": "string",
              "pattern": "\\d{3}",
              "default": "RG10"
            },
            "codePart2": {
              "type": "string",
              "pattern": "\\d{3}",
              "default": "1LG"
            }
          }
        }
      },
      "required": ["street", "city", "state", "postalCode"]
    },
    "hobbies": {
      "type": "array",
      "items": {
        "type": "string",
        "default": ["fishing", "triathlon"]
      }
    },
    "luckyNumbers": {
      "type": "array",
      "items": {
        "type": "integer",
        "default": [77, 39]
      }
    },
    "emptyInts": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "coinToss": {
      "type": "array",
      "items": {
        "type": "boolean",
        "default": [true, false, true]
      }
    },
    "yearsInHouse": {
      "type": "number",
      "default": 16.9,
      "minimum": 0.0
    },
    "meterReadings": {
      "type": "array",
      "items": {
        "type": "number",
        "default": [
          16.9,
          120.9,
          200.64
        ]
      }
    },
    "keepsakes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "jewelery": {
            "type": "object",
            "properties": {
              "metal": {
                "type": "string",
                "default": "Gold"
              },
              "stone": {
                "type": "string",
                "default": "Saphire"
              }
            }
          },
          "book": {
            "type": "object",
            "properties": {
              "metal": {
                "type": "string",
                "default": "Gold"
              },
              "stone": {
                "type": "string",
                "default": "Saphire"
              }
            }
          }
        }
      }
    }
  },
  "required": ["name", "age"]
}
