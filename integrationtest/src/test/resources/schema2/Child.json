{
  "$id": "https://example.com/complex-object.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Person",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "sample": "Sam"
    },
    "age": {
      "type": "integer",
      "sample": 15,
      "minimum": 0
    },
    "address": {
      "type": "object",
      "properties": {
        "street": {
          "type": "string",
          "sample": "Samphire Close"
        },
        "city": {
          "type": "string",
          "sample": "Solihull"
        },
        "state": {
          "type": "string",
          "sample": "West Midlands"
        },
        "postalCode": {
          "type": "object",
          "properties": {
            "codePart1": {
              "type": "string",
              "pattern": "\\d{3}",
              "sample": "B94"
            },
            "codePart2": {
              "type": "string",
              "pattern": "\\d{3}",
              "sample": "2PX"
            }
          }
        }
      },
      "required": ["street", "city", "state", "postalCode"]
    },
    "hobbies": {
      "type": "array",
      "items": {
        "type": "string",
        "sample": ["xbox", "more xbox"]
      }
    },
    "luckyNumbers": {
      "type": "array",
      "items": {
        "type": "integer",
        "sample": [3, 29]
      }
    }
  },
  "required": ["name", "age"]
}
